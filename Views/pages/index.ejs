      <div class="mdc-drawer__content">
        <div class="user-info">
          <p class="name">Dar Sambrano</p>
          <p class="email">darsam44@gmail.com</p>
          <br></br>
          <p class="name">Avital Pikovsky</p>
          <p class="email">avitalpikovsky@gmail.com</p>
        </div>
         <div class="mdc-list-group">
          <nav class="mdc-list mdc-drawer-menu">
            <div class="mdc-list-item mdc-drawer-item">
              <a class="mdc-drawer-link" href="/">
                <i class="material-icons mdc-list-item__start-detail mdc-drawer-item-icon" aria-hidden="true">home</i>
                Dashboard
              </a>
            </div>
            <div class="mdc-list-item mdc-drawer-item">
              <a class="mdc-drawer-link" href="/bigml">
                <i class="material-icons mdc-list-item__start-detail mdc-drawer-item-icon" aria-hidden="true">track_changes</i>
                BigML
              </a>
            </div>
            <img src="./images/dashboard/nodejs.png" style = "width: 80px; padding-left: 10px;">
            <img src="./images/dashboard/kafka.png" style = "width: 130px; padding-right: 10px;">
            <br>
            <img src="./images/dashboard/bigml.png" style = "width: 100px; padding-left: 10px;">
            <img src="./images/dashboard/docker.png" style = "width: 100px; padding-right: 10px;">
            <br>
            <br>
            <img src="./images/dashboard/mongo.png" style = "width: 100px; padding-left: 10px;">
            <img src="./images/dashboard/Redis.png" style = "width: 100px; padding-right: 10px;">
      </div>
    </aside>
    
    <!-- partial -->
    <div class="main-wrapper mdc-drawer-app-content">
      <!-- partial:partials/_navbar.html -->
      <header class="mdc-top-app-bar">
        <div class="mdc-top-app-bar__row">
          <div class="mdc-top-app-bar__section mdc-top-app-bar__section--align-start">
            <button class="material-icons mdc-top-app-bar__navigation-icon mdc-icon-button sidebar-toggler">menu</button>
            <span class="mdc-top-app-bar__title">Welcome to toll road!
            <h6>Click on the cards to see details</h6>
          </span>
          </div>
          <div class="mdc-top-app-bar__section mdc-top-app-bar__section--align-end mdc-top-app-bar__section-right">
            <div class="menu-button-container menu-profile d-none d-md-block">
              <button class="mdc-button mdc-menu-button">
                <span class="d-flex align-items-center">
                  <span class="user-name">Dar Sambrano</span>
                </span>
              </button>
            </div>
        </div>
      </header>
      <!-- partial -->
      <div class="page-wrapper mdc-toolbar-fixed-adjust">
        <main class="content-wrapper">
          <div class="mdc-layout-grid">
            <div class="mdc-layout-grid__inner">
           
              <% all.cards.forEach(function(card) { %>
                <div class="mdc-layout-grid__cell stretch-card mdc-layout-grid__cell--span-3-desktop mdc-layout-grid__cell--span-4-tablet">
                  <%- include ('../partials/Cards.ejs',{car:card}) %>
