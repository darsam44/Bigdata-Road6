                    <h6 class="title"> <i class="mdi mdi-bell-outline mr-2 tx-16"></i><%= card.section %> </h6>
                    <ul class="mdc-list" role="menu" aria-hidden="true" aria-orientation="vertical">
                  <%- include ('../partials/allcars.ejs',{all_sections:card}) %>
              </ul>
            </div>
                </div>
                <% }); %>
              <div class="mdc-layout-grid__cell stretch-card mdc-layout-grid__cell--span-12">
                <div class="mdc-card">
                  <div class="d-flex justify-content-between">
                    <h4 class="card-title mb-0">Brands</h4>
                  </div>
                  <div class="d-block d-sm-flex justify-content-between align-items-center">
                      <h5 class="card-sub-title mb-2 mb-sm-0">vehicles based by brands</h5>
