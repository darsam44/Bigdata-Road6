<body>
<script src="./js/preloader.js"></script>
<script type="text/javascript" src="https://www.gstatic.com/charts/loader.js"></script>
<script type="text/javascript">
  window.onload = function () {
  
  var chart = new CanvasJS.Chart("chartContainer", {
    theme: "light1", // "light2", "dark1", "dark2"
    animationEnabled: false, // change to true		
    title:{
      text: "Count of vehicles evrey day"
    },
    data: [
    {
      // Change type to "bar", "area", "spline", "pie",etc.
      type: "column",
      dataPoints: [
        { label: "Sunday",  y: parseInt('<%=all.Days[0].day%>', 10)  },
        { label: "Monday", y: parseInt('<%=all.Days[1].day%>', 10)  },
        { label: "Tuesday", y: parseInt('<%=all.Days[2].day%>', 10)  },
        { label: "Wednesday",  y: parseInt('<%=all.Days[3].day%>', 10)  },
        { label: "Thursday",  y: parseInt('<%=all.Days[4].day%>', 10)  },
        { label: "Friday",  y: parseInt('<%=all.Days[5].day%>', 10)  },
        { label: "Saturday",  y: parseInt('<%=all.Days[6].day%>', 10)  }
      ]
    }
    ]
  });
  chart.render();
  }
  </script>
  
  <div class="body-wrapper">

    <!-- partial:partials/_sidebar.html -->
